    var submitCreateNewAccount1 = function(event) {
        $('.arrow-up').hide();
        $('.resumeBuilder-icon-2 .arrow-up').show();
        $("div.form-error").hide();
        if (($.trim($("#first_name").val()) == "") || ($.trim($("#last_name").val()) == "")) {
            setError("user_name", "Please, enter your full name.");
        } else if (!myRegEx.test($("#first_name").val()) || !myRegEx.test($("#last_name").val())) {
            setError("user_name", "Please enter your full name. Valid characters include A-z, space, '_' underscore, and '-' dash.");
        } else {
            location.hash = "#Step3";
            $('#createaccountCarousel').carousel(2);
        }
    }

    var submitCreateNewAccount2 = function(event) {
        $('.arrow-up').hide();
        $('.resumeBuilder-icon-2 .arrow-up').show();
        $("div.form-error").hide();
        $("div").removeClass("has-error");

        var email = $("#email").val().toLowerCase();
        if (($.trim(email) != "") && isEmail(email)) {
            location.hash = "#Step4";
            $('#createaccountCarousel').carousel(3);
        } else {
            setError("email", "Please enter a valid email");
        }
    }

    var submitCreateNewAccount3 = function(event) {
        var minErrorStep = -1;
        $('.arrow-up').hide();
        $('.resumeBuilder-icon-2 .arrow-up').show();
        $("div.form-error").hide();
        var email = $("#email").val().toLowerCase();

        //CHECK JC
        if ($.trim($("#jobcategory_input").val()) == "") {
            errorMessage = "Please enter a work industry";
            className = "job_category";
            minErrorStep = 3;
            setError(className, errorMessage);
        }
        //CHECK EMAIL
        if (($.trim(email) == "")) {
            minErrorStep = 2;
        }

        if (!isEmail(email)) {
            minErrorStep = 2;
        }
        //CHECK NAME
        if (($.trim($("#first_name").val()) == "") || ($.trim($("#last_name").val()) == "")) {
            minErrorStep = 1;
        } else if (!myRegEx.test($("#first_name").val()) || !myRegEx.test($("#last_name").val())) {
            minErrorStep = 1;
        }

        if (minErrorStep > 0) {
            console.log('should stop');
            Ladda.stopAll();
            location.hash = "#Step" + (minErrorStep + 1);
            if (minErrorStep == 1) {
                setError("user_name", "Please, enter your full name.");
            } else if (minErrorStep == 2) {
                setError("email", "Please enter a valid email");
            }
            $('#createaccountCarousel').carousel(minErrorStep);
        } else {
            var post = {
                "fName": $('#first_name').val(),
                "lName": $('#last_name').val(),
                "email": $('#email').val(),
                "jcategorys": $('#jobcategory_input').val(),
                "templateID": $('.currentSelectedTemplate').data("template-id") || $.getUrlVar('template') || 1,
                "agreePage": "https://www.resume.com/builder",
            }

            //console.log('post data', post);
            $.ajax({
                type: "POST",
                url: "/user/create",
                data: post,
                success: function(data, textStatus, jqXHR) {
                    Ladda.stopAll();
                    window.location.href = "/dashboard/" + data.emailHash + "?wizard=true";
                },
                error: function(XMLHttpRequest, textStatus, errorThrown) {
                    Ladda.stopAll();
                    if (XMLHttpRequest.status == 403) {
                        setError("email", "Email already exists.");
                        location.hash = "#Step3";
                    } else {
                        setError("job_category", "Sorry, something went wrong. Please, try again later or contact support@resume.com. " + errorThrown + " - " + textStatus);
                    }
                    console.log(textStatus);
                    console.log(errorThrown);
                },
                dataType: 'json'
            });
        }
    };

    var myRegEx = /^([a-zA-Z0-9 _-]+)$/;

    function setError(className, errorMessage) {
        if (className && errorMessage) {
            $(".err" + className + "msg").show();
            $("input." + className).parent().addClass("has-error");
            $(".err" + className + "msgText").empty().html(errorMessage);
        }
        $(".err" + className + "msgText").empty().html(errorMessage);
    }

    isEmail = function(email) {
        var regex = /^([*+!.&#$В¦\'\\%\/0-9a-z^_`{}=?~:-]+)@(([0-9a-z-]+\.)+[0-9a-z]{2,4})$/i;
        return regex.test(email);
    };


	(function($){
        $.getUrlVar = function(key){
            var result = new RegExp(key + "=([^&]*)", "i").exec(window.location.search);
            return result && unescape(result[1]) || "";
        };
    })(jQuery);

